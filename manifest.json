{
    "manifest_version": 2,
    "name": "Kodi video opener",
    "description": "This extension starts films on kodi",
    "version": "1.2",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest", "tabs", "storage", "*://*/*"
    ],
    "background": {
        "scripts": ["jquery.js", "script.js"]
    },
    "icons": {
        "16": "icons/normal/icon16.png",
        "32": "icons/normal/icon32.png",
        "48": "icons/normal/icon48.png",
        "64": "icons/normal/icon64.png",
        "128": "icons/normal/icon128.png"
    },
    "options_page": "options.html",
    "key": ""
}
